<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mathgame">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <uses-feature
            android:name="android.hardware.camera"
            android:required="true" />

        <activity android:name=".ActivitySignUp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Drill1Activity" />
        <activity android:name=".Drill2Activity" />
        <activity android:name=".Drill3Activity" />
        <activity android:name=".GameOverActivity">
        </activity>
        <activity android:name=".ActivityEnd">
        </activity>
    </application>

</manifest>